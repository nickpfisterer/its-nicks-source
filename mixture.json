{
    "debug": false,
    "lintScripts": false,
    "preprocessorLocations": [ "assets" ],
    "preprocessorScriptOutput": null,
    "preprocessorStyleOutput": "assets/css",
    "projectName": "Nick's Space",
    "projectDescription": "Source code for my blog & portfolio site",
    "publishedId": null,
    "serverHostname": null,
    "serverPort": null,
    "templateDirectory": "templates",
    "templateDefaultLayout": "post",
    "templateDefaultPage": "index",
    "type": "mixture",
    "useCompass": false,
    "simpleModePartnerServer": null,
    "simpleModeFileTypes": null,
    "modelSource": null,
    "domain": null,
    "followLeader": true,
    "customRubyPath": null,
    "remoteDebugPort": 0,
    "optimiseImages": true,
    "convertHtml": { "relativePaths": false },
    "sass": {
      "sourceMap": false,
      "lineNumbers": false,
      "useLibSass": true
    },
    "autoPrefixer": {
      "active": true,
      "sourceMap": false,
      "browsers": [ "last 2 versions" ]
    },
    "routes": [
        {
            "route": "page-:page",
            "template": "index",
            "redirect": null
        },
        {
            "route": "tag/:tag",
            "template": "tag",
            "redirect": null
        }
    ]
}
